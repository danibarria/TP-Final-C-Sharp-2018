<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.data>
    <DbProviderFactories>
      <add name="Npgsql Data Provider" invariant="Npgsql" description="Data Provider for PostgreSQL" type="Npgsql.NpgsqlFactory, Npgsql" />
    </DbProviderFactories>
  </system.data>    
  <connectionStrings>
    <add name="pgsqlContext" connectionString="Server=localhost;Port=5432;Database=turnos;User Id=postgres;Password=master;" providerName="Npgsql" />
  </connectionStrings>
</configuration>